{
  "name": "nyc-manhattan-reachability-map",
  "version": "1.0.0",
  "description": "Interactive reachability map for NYC Manhattan using Leaflet.js and isochrone analysis",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 8000 -o",
    "serve": "python -m http.server 8000",
    "dev": "npx live-server --port=8000 --open=/index.html"
  },
  "keywords": [
    "leaflet",
    "maps",
    "isochrone",
    "reachability",
    "nyc",
    "manhattan",
    "geospatial",
    "transportation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nyc-reachability-map.git"
  },
  "homepage": "https://yourusername.github.io/nyc-reachability-map"
}
